                                        (0491)    cmp  A,UART_INT_MODE_TX_COMPLETE
                                        (0492)    jz   .SetTxCmpMode
                                        (0493) 
                                        (0494)    and  reg[UART_TX_FUNC_REG],0xEF               ; Set Interrupt on Tx_Reg_Empty
                                        (0495)    jmp  .TxIntMode_End
                                        (0496) 
